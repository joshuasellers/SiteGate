{
  "name": "SiteGate",
  "version": "1.0",
  "description": "Makes it harder to enter websites that you can't stay away from.",
  "permissions": ["activeTab", "tabs", "webNavigation"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html",
  },
  "manifest_version": 2
}